FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      k;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -2 0 0 0 0];

internalField   uniform 3.75e-05; // Low turbulence intensity

boundaryField
{
    top
    {
        type            inletOutlet;
        inletValue      uniform 3.75e-05;
        value           uniform 3.75e-05;
    }
    bottom
    {
        type            kqRWallFunction;
        value           uniform 3.75e-05;
    }
    walls
    {
        type            kqRWallFunction;
        value           uniform 3.75e-05;
    }
}

// ************************************************************************* //
