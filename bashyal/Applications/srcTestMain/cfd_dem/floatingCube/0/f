FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    object      f;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -2 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    top
    {
        type            calculated;
        value           uniform (0 0 0);
    }

    bottom
    {
        type            calculated;
        value           uniform (0 0 0);
    }

    walls
    {
        type            calculated;
        value           uniform (0 0 0);
    }
}

